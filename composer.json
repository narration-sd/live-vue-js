{
    "name": "craftcms/craft",
    "description": "Craft CMS",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "project"
    ],
    "minimum-stability": "beta",
    "license": "MIT",
    "homepage": "https://craftcms.com/",
    "type": "project",
    "support": {
        "email": "support@craftcms.com",
        "issues": "https://github.com/craftcms/cms/issues",
        "forum": "https://craftcms.stackexchange.com/",
        "source": "https://github.com/craftcms/cms",
        "docs": "https://craftcms.com/docs",
        "rss": "https://craftcms.com/changelog.rss"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:narrationsd/live-vue.git"
        },
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:narrationsd/live-vue-js.git"
        }
    ],
    "require": {
        "craftcms/cms": "3.1.17",
        "craftcms/element-api": "^2.5",
        "craftcms/redactor": "^2.3",
        "markhuot/craftql": "2.x",
        "narrationsd/live-vue": "dev-gatsby",
        "narrationsd/live-vue-js": "dev-gatsby",
        "vlucas/phpdotenv": "^2.4.0"
    },
    "require-dev": {
        "laravel/homestead": "^8.0",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
    "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.1.3"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
